<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title></title>
            <script>
            Handlebars.registerHelper('list', function(items, options) {
                var out = '';
                for (var i = 0, l = items.length; i < l; i++) {
                    out = out + options.fn(items[i]);
                };
                return out;
            });
            </script>
    </head>
    <body>
        var priceList = {
            {{#list items}}
            "{{ id }}": {
                "id": "{{ id }}",
                "subid": {},
                "name": "{{ name }}",
                "price": "{{ price }}"
            },
            {{/list}}
        };
    </body>
</html>
